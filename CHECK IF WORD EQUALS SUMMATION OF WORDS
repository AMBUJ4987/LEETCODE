class Solution:
    def isSumEqual(self, firstWord: str, secondWord: str, targetWord: str) -> bool:
        num1 = ""
        for c in firstWord:
            num1 += str(ord(c) - ord('a'))

        num2 = ""
        for c in secondWord:
            num2 += str(ord(c) - ord('a'))

        num3 = ""
        for c in targetWord:
            num3 += str(ord(c) - ord('a'))

        return int(num1) + int(num2) == int(num3)

