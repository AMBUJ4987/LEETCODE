class Solution:
    def findMaxLength(self, nums: List[int]) -> int:
        d = {0: -1}
        s = 0
        m = 0
        for i in range(len(nums)):
            s += 1 if nums[i] == 1 else -1
            if s in d:
                m = max(m, i - d[s])
            else :
                d[s] = i
        return m        




        
